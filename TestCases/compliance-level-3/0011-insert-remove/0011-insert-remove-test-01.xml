<?xml version="1.0" encoding="UTF-8"?>
<testCases xmlns="http://www.omg.org/spec/DMN/20160719/testcase" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.omg.org/spec/DMN/20160719/testcase ../testCases.xsd">
	<modelName>0011-insert-remove.dmn</modelName>
	<testCase id="001">
		<inputNode name="simpleList">
			<list>
				<item>
					<value>a</value>
				</item>
				<item>
					<value>b</value>
				</item>
				<item>
					<value>c</value>
				</item>
			</list>
		</inputNode>
		<inputNode name="nestedList">
			<list>
				<item>
					<value>o</value>
				</item>
				<item>
					<list>
						<item>
							<value>p</value>
						</item>
						<item>
							<value>q</value>
						</item>
					</list>
				</item>
				<item>
					<value>r</value>
				</item>
			</list>
		</inputNode>
		<inputNode name="position">
			<value>2</value>
		</inputNode>
		<resultNode name="literalNestedList" type="decision">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
					<item>
						<list>
							<item>
								<value>b</value>
							</item>
							<item>
								<value>c</value>
							</item>
						</list>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="remove1" type="decision">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
					<item>
						<value>c</value>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="insert1" type="decision">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
					<item>
						<list>
							<item>
								<value>o</value>
							</item>
							<item>
								<list>
									<item>
										<value>p</value>
									</item>
									<item>
										<value>q</value>
									</item>
								</list>
							</item>
							<item>
								<value>r</value>
							</item>
						</list>
					</item>
					<item>
						<list>
							<item>
								<value>b</value>
							</item>
							<item>
								<value>c</value>
							</item>
						</list>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="insert2" type="decision">
			<expected>
				<list>
					<item>
						<value>o</value>
					</item>
					<item>
						<list>
							<item>
								<value>a</value>
							</item>
							<item>
								<list>
									<item>
										<value>b</value>
									</item>
									<item>
										<value>c</value>
									</item>
								</list>
							</item>
						</list>
					</item>
					<item>
						<list>
							<item>
								<value>p</value>
							</item>
							<item>
								<value>q</value>
							</item>
						</list>
					</item>
					<item>
						<value>r</value>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="remove2" type="decision">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
				</list>
			</expected>
		</resultNode>
		<resultNode name="insert3" type="decision">
			<expected>
				<list>
					<item>
						<value>a</value>
					</item>
					<item>
						<value>x</value>
					</item>
					<item>
						<value>b</value>
					</item>
					<item>
						<value>c</value>
					</item>
				</list>
			</expected>
		</resultNode>
	</testCase>
</testCases>
